<Window x:Class="GiaTest2.AddWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GiaTest2"
        mc:Ignorable="d"
        Title="AddWindow" Height="450" Width="800">
    <Grid>
        <Grid Margin="120,80,120,20">
            <TextBox Name="PartnertypeBox"  HorizontalAlignment="Center" Margin="0,19,0,0" TextWrapping="Wrap" Text="Тип партнера" VerticalAlignment="Top" Width="372" RenderTransformOrigin="-0.109,1.098" Height="24"/>
            <TextBox Name="PartnernameBox" HorizontalAlignment="Center" Margin="0,50,0,0" TextWrapping="Wrap" Text="Имя партнера" VerticalAlignment="Top" Width="372" Height="24"/>
            <TextBox Name="DirectorBox" HorizontalAlignment="Center" Margin="0,81,0,0" TextWrapping="Wrap" Text="ФИО Дирекора" VerticalAlignment="Top" Width="372" Height="24"/>
            <TextBox Name="EmailBox" HorizontalAlignment="Center" Margin="0,112,0,0" TextWrapping="Wrap" Text="Эллектронная почта" VerticalAlignment="Top" Width="372" Height="25"/>
            <TextBox Name="PhoneBox" HorizontalAlignment="Center" Margin="0,143,0,0" TextWrapping="Wrap" Text="Телефон" VerticalAlignment="Top" Width="372" Height="25"/>
            <TextBox Name="AddressBox" HorizontalAlignment="Center" Margin="0,174,0,0" TextWrapping="Wrap" Text="Адресс" VerticalAlignment="Top" Width="372" Height="25"/>
            <TextBox Name="InnBox" HorizontalAlignment="Center" Margin="0,206,0,0" TextWrapping="Wrap" Text="ИНН" VerticalAlignment="Top" Width="372" Height="24"/>
            
            


        </Grid>
        <Button Content="Отменить" Click="DenyClick" HorizontalAlignment="Left" Margin="599,80,0,0" VerticalAlignment="Top" Height="38" Width="81"/>
        <Button Content="Сохранить" Click="SaveClick" HorizontalAlignment="Left" Margin="120,80,0,0" VerticalAlignment="Top" Height="38" Width="81"/>
    </Grid>
</Window>
